		<servlet-name>IdiomaServiceServlet</servlet-name>
		<servlet-class>com.sitep.str.integration.in.servlets.IdiomaServiceServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>IdiomaServiceServlet</servlet-name>
		<url-pattern>/IdiomaServiceServlet</url-pattern>
	</servlet-mapping>
	
