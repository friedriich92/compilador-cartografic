<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

	<display-name>STRIntegration</display-name>

	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>classpath*:/applicationContext.xml</param-value>
	</context-param>

	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>
	
	<servlet>
		<servlet-name>FonsOfflineServiceServlet</servlet-name>
		<servlet-class>com.sitep.str.integration.in.servlets.FonsOfflineServiceServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>FonsOfflineServiceServlet</servlet-name>
		<url-pattern>/FonsOfflineServiceServlet</url-pattern>
	</servlet-mapping>
	
